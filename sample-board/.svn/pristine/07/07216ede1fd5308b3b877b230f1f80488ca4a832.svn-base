package com.webjjang.main.model;

import java.util.List;

import com.webjjang.goods.model.Goods;
import com.webjjang.notice.model.Notice;
import com.webjjang.qna.model.Qna;

public class MainListView {
	
	public MainListView(){}
	public MainListView(List<Notice> noticeList, List<Qna> qnaList,List<Goods> goodsList){
		// 생성하면선 값을 셋팅 : 미리 값을 구해 놓는다.
		this.noticeList = noticeList;
		this.qnaList = qnaList;
		this.goodsList = goodsList;
	}
	
	private List<Qna> qnaList;
	private List<Notice> noticeList;
	private List<Goods> goodsList;
	public List<Qna> getQnaList() {
		return qnaList;
	}
	public void setQnaList(List<Qna> qnaList) {
		this.qnaList = qnaList;
	}
	public List<Notice> getNoticeList() {
		return noticeList;
	}
	public void setNoticeList(List<Notice> noticeList) {
		this.noticeList = noticeList;
	}
	public List<Goods> getGoodsList() {
		return goodsList;
	}
	public void setGoodsList(List<Goods> goodsList) {
		this.goodsList = goodsList;
	}
}
